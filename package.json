{
  "name": "@aquaxyz/identity-sdk",
  "packageManager": "yarn@3.2.4",
  "version": "1.0.0",
  "description": "A typescript library using aqua identity authentication.",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "eslint": "eslint --cache --fix .",
    "eslint:check": "eslint .",
    "build": "tsc -b tsconfig.build.json",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tsconfig/node14": "^1.0.3",
    "query-string": "^6.12.1",
    "events": "^3.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aquaxyz/identity-sdk.git"
  },
  "keywords": [
    "aquaxyz",
    "identity"
  ],
  "bugs": {
    "url": "https://github.com/aquaxyz/identity-sdk/issues"
  },
  "homepage": "https://github.com/aquaxyz/identity-sdk#readme"
